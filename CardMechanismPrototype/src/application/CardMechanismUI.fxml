                  <Button fx:id="revealAction2" mnemonicParsing="false" onAction="#action2" onMouseClicked="#action2" prefHeight="25.0" prefWidth="192.0" text="Revote" />
               </children>
            </VBox>
      </children>
    </AnchorPane>
  </children>
</VBox>
